{
  "manifest_version": 2,

  "name": "Github Hot Keys",
  "description": "This extension gives you some more hotkeys",
  "version": "1.0",
  "content_scripts": [
    {
        "js": ["jquery.min.js", "hotkeys.js"],
        "matches": [
          "https://github.com/Workiva/*/pull*",
          "https://github.com/workiva/*/pull*"
        ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
